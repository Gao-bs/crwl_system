<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.system.mapper.CrwlResultMapper" >
  <resultMap id="BaseResultMap" type="com.system.entity.pojo.CrwlResult" >
    <!--
      WARNING - @mbggenerated
    -->
    <id column="id" property="id" jdbcType="VARCHAR" />
    <result column="gjc" property="gjc" jdbcType="VARCHAR" />
    <result column="url" property="url" jdbcType="VARCHAR" />
     <result column="user_name" property="userName" jdbcType="VARCHAR" />
    <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
    <result column="title" property="title" jdbcType="VARCHAR" />
    <result column="type" property="type" jdbcType="VARCHAR" />
    <result column="contents" property="contents" jdbcType="LONGVARBINARY" />
  </resultMap>
  
  
  <select id="getStaticsCrwlResultList" resultType="com.system.entity.dto.CrwlResultDto"  parameterType="com.system.entity.pojo.CrwlResult" >
  
  select  tr.gjc,count(*) as  count    from  t_result  tr  
  
    <where>
    <if test="gjc!=null and gjc!=''">
     and tr.gjc  like CONCAT(CONCAT('%',#{gjc}),'%')
    </if>
    </where>
  
  group by  tr.gjc
    </select>
    <select id="selectBySelective" resultMap="BaseResultMap"  parameterType="com.system.entity.pojo.CrwlResult" >
    select p.* from t_result p
    <where>
    <if test="gjc!=null and gjc!=''">
     and p.gjc  like CONCAT(CONCAT('%',#{gjc}),'%')
    </if>
    </where>
    order  by create_time desc
  </select>
  
</mapper>